<% if @notification[:notice] && @abuse_report.processed %>
  $('tr.abuse-report-' + <%= @abuse_report.id %> ).find('.approve-form').remove();
  $('tr#abuse-report-' + <%= @abuse_report.id %> ).find('i.fa-question-circle').remove();
  toastr.success('<%= @notification[:notice] %>');
<% else %>
  toastr.error('<%= @notification[:alert] %>');
<% end %>